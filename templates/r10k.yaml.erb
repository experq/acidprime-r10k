---
<% if @postrun and @postrun.is_a?(Array) -%>
:postrun: [<%= @postrun.map{ |s| "\"#{s}\"" }.join(',') %>]
<% end -%>
:cachedir: <%= @cachedir %>
<% unless @r10k_sources.empty? -%>
:git:
<% @git.sort.each do |key,value| -%>
  <%=key-%>: <%=value%>
:sources:
<% @source_keys.sort.each do |source| -%>
  <%=source-%>:
<% @r10k_sources[source].sort.each do |key,value| -%>
    <%=key-%>: <%=value%>
<% end -%>
<% end -%>
<% end %>
